<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]   MyFace</title>


    <link rel="stylesheet" href="~/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/style.css" />
    <link rel="stylesheet" href="~/css/ionicons.min.css" />
    <link rel="stylesheet" href="~/css/font-awesome.min.css" />
    <link href="~/css/emoji.css" rel="stylesheet">
    <!--Google Webfont-->
    <link href='https://fonts.googleapis.com/css?family=Raleway:400,100,100italic,200,200italic,300,300italic,400italic,500,500italic,600,600italic,700' rel='stylesheet' type='text/css'>
    <link rel="shortcut icon" type="image/png" href="~/images/logohead.png" />
    @RenderSection("ProfilnaSlikaPozadina", required: false)
</head>
<body>
    <header id="header">
        <nav class="navbar navbar-default navbar-fixed-top menu">
            <div class="container">

                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="index.html"><img src="~/images/logo.png" alt="logo" height="25px" width="100px" /></a>
                </div>
                @*../Pages/*@
                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                    <ul class="nav navbar-nav navbar-right main-menu">
                        <li class="dropdown"><a asp-page="/Pocetna stranica/VremenskaLinija">Home</a></li>
                        <li class="dropdown"><a asp-page="/Pocetna stranica/PronadjiPrijatelje">Search</a></li>
                        <li class="dropdown"><a asp-page="/Pocetna stranica/IzmeniProfil">Change my face</a></li>
                        <li class="dropdown"><a asp-page="/Prijatelji/Chat">Chat</a></li>
                        <li class="dropdown"><a asp-page="#">Change account</a></li>
                        <li class="dropdown"><a asp-page="#">Logout</a></li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <!--Header End-->
    <div class="timeline-cover">

        <!--Timeline Menu for Large Screens-->
        <div class="timeline-nav-bar hidden-sm hidden-xs">
            <div class="row">
                <div class="col-md-3">
                    <div class="profile-info">
                        @RenderSection("ProfilnaSlika", required: false)
                        <h4>@RenderSection("ImeIPrezime", required: false)</h4>
                    </div>
                </div>
                <div class="col-md-9">
                    <ul class="list-inline profile-menu">
                        <li><a asp-page="/Pocetna stranica/VremenskaLinija" class="active">Timeline</a></li>
                        <li><a asp-page="/Pocetna stranica/Objavi">Share</a></li>
                        <li><a asp-page="/Pocetna stranica/MojProfil">My face</a></li>
                        <li class="dropdown">
                            <a asp-page="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Friends <span><img src="images/down-arrow.png" alt="" /></span></a>
                            <ul class="dropdown-menu login">
                                <li><a asp-page="/Prijatelji/ListaBlokiranihPrijatelja">Blocked</a></li>
                                <li><a asp-page="/Prijatelji/ListaPrijatelja">Others</a></li>
                            </ul>
                        </li>
                        <li><a asp-page="/Prijatelji/Obavestenja">Notifications</a></li>
                        <li><a asp-page="/Stranica/ListaLajkovanihStranica">Liked pages</a></li>
                    </ul>
                    <ul class="follow-me list-inline">
                        <li>@RenderSection("BrojPratilaca", required: false)</li>
                    </ul>
                </div>
            </div>
        </div><!--Timeline Menu for Large Screens End-->
        <!--Timeline Menu for Small Screens-->
        <div class="navbar-mobile hidden-lg hidden-md">
            <div class="profile-info">
                <img src=@RenderSection("ProfilnaSlika2", required: false) alt="" class="img-responsive profile-photo" />
                <h4>@RenderSection("ImeIPrezime2", required: false)</h4>
            </div>
            <div class="mobile-menu">
                <ul class="list-inline">
                    <li><a asp-page="/Pocetna stranica/VremenskaLinija" class="active">Timeline</a></li>
                    <li><a asp-page="/Pocetna stranica/Objavi">Share</a></li>
                    <li><a asp-page="/Pocetna stranica/MojProfil">My face</a></li>
                    <li class="dropdown">
                        <a asp-page="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Friends <span><img src="images/down-arrow.png" alt="" /></span></a>
                        <ul class="dropdown-menu login">
                            <li><a asp-page="/Prijatelji/ListaBlokiranihPrijatelja">Blocked</a></li>
                            <li><a asp-page="/Prijatelji/ListaPrijatelja">Others</a></li>
                        </ul>
                    </li>
                    <li><a asp-page="/Prijatelji/Obavestenja">Notifications</a></li>
                    <li><a asp-page="/Stranica/ListaLajkovanihStranica">Liked pages</a></li>
                </ul>
            </div>
        </div><!--Timeline Menu for Small Screens End-->
    </div>
        <div class="container body-content">
            @RenderBody()
            <hr />
            <footer>
                <p>&copy; 2021 - MyFace</p>
            </footer>
        </div>

        <script src="~/js/jquery-3.1.1.min.js"></script>
        <script src="~/js/bootstrap.min.js"></script>
        <script src="~/js/jquery.sticky-kit.min.js"></script>
        <script src="~/js/jquery.scrollbar.min.js"></script>
        <script src="~/js/script.js"></script>
        <script src="~/js/site.min.js" asp-append-version="true"></script>

        @RenderSection("Scripts", required: false)
</body>
</html>
